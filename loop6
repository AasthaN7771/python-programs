def print_time_suffixes():
    for hour in range(24):
        if hour == 0:
            print("12:00 AM")
        elif hour < 12:
            print(f"{hour}:00 AM")
        elif hour == 12:
            print("12:00 PM")
        else:
            print(f"{hour - 12}:00 PM")


print_time_suffixes()